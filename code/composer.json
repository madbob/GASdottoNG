{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1",
        "ext-gd": "*",
        "aws/aws-sdk-php": "^3.38",
        "barryvdh/laravel-debugbar": "^3.1",
        "belphemur/laravel-gettext": "7.x",
        "devfactory/minify": "^1.0",
        "doctrine/dbal": "^2.5",
        "elibyy/tcpdf-laravel": "^5.4",
        "fideloper/proxy": "~4.0",
        "laravel/framework": "5.6.*",
        "laravel/tinker": "~1.0",
        "madbob/laravel-log2rss": "^0.1.0",
        "paypal/rest-api-sdk-php": "^1.13",
        "satispay/online-api-php-sdk": "^1.6",
        "sinergi/browser-detector": "^6.1",
        "spatie/laravel-feed": "^2.0",
        "vespakoen/menu": "^3.0",
        "wilgucki/csv": "^0.6.2",
        "zetacomponents/archive": "^1.5"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~7.0",
        "filp/whoops": "~2.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Helpers/Setup.php",
            "app/Helpers/Formatters.php",
            "app/Helpers/Reflection.php",
            "app/Helpers/Paths.php",
            "app/Helpers/Files.php",
            "app/Helpers/Locale.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
