{
    "name": "madbob/gasdottong",
    "description": "Gestionale per i Gruppi di Acquisto Solidale",
    "keywords": ["management", "money", "fair economy"],
    "license": "AGPL-3.0+",
    "type": "project",
    "require": {
        "php": ">=7.1",
        "ext-gd": "*",
        "aws/aws-php-sns-message-validator": "^1.5",
        "aws/aws-sdk-php": "^3.67",
        "barryvdh/laravel-debugbar": "^3.2",
        "barryvdh/laravel-dompdf": "^0.8.4",
        "devfactory/minify": "^1.0",
        "doctrine/dbal": "^2.8",
        "eluceo/ical": "^0.14.0",
        "fideloper/proxy": "~4.0",
        "laravel/framework": "^6.0",
        "laravel/tinker": "~1.0",
        "league/csv": "^9.6",
        "madbob/laravel-log2rss": "^0.3.0",
        "paypal/rest-api-sdk-php": "^1.13",
        "satispay/online-api-php-sdk": "^1.6",
        "sinergi/browser-detector": "^6.1",
        "vespakoen/menu": "^3.0",
        "zerospam/laravel-gettext": "7.x",
        "zetacomponents/archive": "^1.5"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~7.0",
        "filp/whoops": "~2.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Helpers/Setup.php",
            "app/Helpers/Formatters.php",
            "app/Helpers/Reflection.php",
            "app/Helpers/Paths.php",
            "app/Helpers/Files.php",
            "app/Helpers/Locale.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
