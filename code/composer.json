{
    "name": "madbob/gasdottong",
    "description": "Gestionale per i Gruppi di Acquisto Solidale",
    "keywords": ["management", "money", "fair economy"],
    "license": "AGPL-3.0+",
    "type": "project",
    "require": {
        "php": ">=8.1",
        "ext-gd": "*",
        "aws/aws-php-sns-message-validator": "^1.5",
        "aws/aws-sdk-php": "^3.67",
        "barryvdh/laravel-debugbar": "^3.7",
        "barryvdh/laravel-dompdf": "^2.0.0",
        "debril/feed-io": "^5.0",
        "deepskylog/laravel-gettext": "^7.4",
        "eluceo/ical": "^2.0",
        "fideloper/proxy": "~4.0",
        "genealabs/laravel-model-caching": "^0.12",
        "haydenpierce/class-finder": "^0.4.3",
        "laravel/framework": "^9.0",
        "laravel/ui": "^4.0",
        "lavary/laravel-menu": "^1.8",
        "league/csv": "^9.6",
        "madbob/larastrap": "dev-master",
        "madbob/laravel-log2rss": "0.5.0",
        "madbob/laravel-queue-loopback": "0.4.0",
        "paypal/rest-api-sdk-php": "^1.13",
        "satispay/online-api-php-sdk": "^1.6",
        "spatie/db-dumper": "^3.0",
        "symfony/http-client": "^6.2",
        "symfony/psr-http-message-bridge": "^2.1",
        "symfony/sendinblue-mailer": "^6.2",
        "zetacomponents/archive": "dev-master"
    },
    "require-dev": {
        "brianium/paratest": "^6.3",
        "fakerphp/faker": "^1.15",
        "spatie/laravel-ignition": "^1.0",
        "laravel/dusk": "^6.15",
        "madbob/dusk-utils": "^0.1.0",
        "mockery/mockery": "~1.0",
        "nunomaduro/collision": "^6.1",
        "nunomaduro/larastan": "^2.3.0",
        "phpunit/phpunit": "^9.3",
        "staudenmeir/dusk-updater": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers/Setup.php",
            "app/Helpers/Formatters.php",
            "app/Helpers/Dates.php",
            "app/Helpers/Percentages.php",
            "app/Helpers/Permissions.php",
            "app/Helpers/Reflection.php",
            "app/Helpers/Paths.php",
            "app/Helpers/Accounting.php",
			"app/Helpers/Orders.php",
            "app/Helpers/Files.php",
            "app/Helpers/Locale.php",
            "app/Helpers/Components.php",
			"app/Helpers/Status.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "barryvdh/laravel-ide-helper"
            ]
        }
    }
}
