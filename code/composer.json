{
    "name": "madbob/gasdottong",
    "description": "Gestionale per i Gruppi di Acquisto Solidale",
    "keywords": ["management", "money", "fair economy"],
    "license": "AGPL-3.0+",
    "type": "project",
    "require": {
        "php": ">=7.3",
        "ext-gd": "*",
        "aws/aws-php-sns-message-validator": "^1.5",
        "aws/aws-sdk-php": "^3.67",
        "barryvdh/laravel-debugbar": "^3.2",
        "barryvdh/laravel-dompdf": "^0.9.0",
        "doctrine/dbal": "^2.8",
        "eluceo/ical": "^0.14.0",
        "fideloper/proxy": "~4.0",
        "genealabs/laravel-model-caching": "^0.11.3",
        "laravel/framework": "^8.0",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^3.0",
        "laravelium/feed": "^8.0",
        "lavary/laravel-menu": "^1.8",
        "league/csv": "^9.6",
        "madbob/larastrap": "dev-master",
        "madbob/laravel-queue-loopback": "^0.3.0",
        "paypal/rest-api-sdk-php": "^1.13",
        "satispay/online-api-php-sdk": "^1.6",
        "sinergi/browser-detector": "^6.1",
        "spatie/db-dumper": "^2.21",
        "zerospam/laravel-gettext": "^8.0.0",
        "zetacomponents/archive": "dev-master"
    },
    "require-dev": {
        "fakerphp/faker": "^1.15",
        "filp/whoops": "~2.0",
        "laravel/dusk": "^6.15",
        "mockery/mockery": "~1.0",
        "nunomaduro/larastan": "^0.7.0",
        "phpunit/phpunit": "^9.3",
        "staudenmeir/dusk-updater": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers/Setup.php",
            "app/Helpers/Formatters.php",
            "app/Helpers/Dates.php",
            "app/Helpers/Percentages.php",
            "app/Helpers/Reflection.php",
            "app/Helpers/Paths.php",
            "app/Helpers/Files.php",
            "app/Helpers/Locale.php",
            "app/Helpers/Components.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "barryvdh/laravel-ide-helper"
            ]
        }
    }
}
